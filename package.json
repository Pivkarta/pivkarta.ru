{
  "name": "@prisma-cms/boilerplate",
  "version": "1.3.3-beta",
  "description": "BETA opensource framework CMS powered by GraphQL, Prisma, MySQL, React & Apollo GraphQL",
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start-ssr": "NODE_ENV=production node src/ssr",
    "start-ssr-dev": "NODE_ENV=production nodemon src/ssr",
    "start-server": "Sendmail=true action=start-server node src/server/",
    "start-server-dev": "Sendmail=true SendmailTest=true action=start-server nodemon src/server/index.js",
    "deploy": "run-s build-schema-prisma && yarn deploy-schema && yarn get-schema -p prisma && yarn build-schema-api",
    "deploy-force": "run-s build-schema-prisma && yarn deploy-schema -f && yarn get-schema -p prisma && yarn build-schema-api",
    "deploy-prod": "yarn deploy",
    "build-schema-prisma": "schemaType=prisma yarn build-schema",
    "deploy-schema": "node_modules/prisma/dist/index.js deploy",
    "get-schema": "node_modules/graphql-cli/dist/bin.js get-schema -p prisma -o src/schema/generated/prisma.graphql",
    "build-schema-api": "schemaType=api yarn build-schema",
    "build-schema": "action=build-schema node src/server/index.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
